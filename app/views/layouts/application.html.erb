<!DOCTYPE html>
<html>
<head>
  <title>Chat</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<nav>
  <% if current_user %>
    <%= link_to "Log out", logout_path, :method => :delete %>
  <% else %>
    <%= link_to "Log in", login_path %>
  <% end %>
</nav>

<%= yield %>

</body>
</html>
